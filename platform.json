{
  "name": "native",
  "title": "Native",
  "description": "Native development platform is intended to be used for desktop OS. This platform uses built-in toolchains (preferable based on GCC), frameworks, libs from particular OS where it will be run.",
  "homepage": "http://platformio.org/platforms/native",
  "license": "Apache-2.0",
  "keywords": [
    "dev-platform",
    "native",
    "desktop"
  ],
  "engines": {
    "platformio": "^5"
  },
  "packages": {
    "framework-ESPtux": {
      "type": "framework",
      "version": "https://github.com/peterus/framework-ESPtux.git",
      "optional": true
    }
  },
  "frameworks": {
    "arduino": {
      "package": "framework-ESPtux",
      "script": "builder/frameworks/arduino.py"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/platformio/platform-native.git"
  },
  "version": "1.1.4"
}
